{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": ["s3:*"],
      "Resource": [
        "arn:aws:s3:::minecraft-server-cg",
        "arn:aws:s3:::minecraft-server-cg/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "route53:*",
        "route53domains:*"
      ],
      "Resource": ["*"]
    }
  ]
}